<%	boolean isAuthenticated = false;	String username = request.getParameter("username");				System.out.println(username);			String pwd = request.getParameter("pwd");				System.out.println(pwd);			isAuthenticated = ( (username.equals("demo")) && (pwd.equals("demo")) );	StringBuffer xml = new StringBuffer("<?xml version=\"1.0\" encoding=\"UTF-8\"?><response>");	if (isAuthenticated)	{		xml.append("<token id=\"010203\"/>");	} else {		xml.append("<token id=\"0\"/>");	}	xml.append("</response>");%><%=xml%>